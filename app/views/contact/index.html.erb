<div id="contact">
<h1>Contact Us</h1>
<p>Fill out the form below, we will get back to you as soon as possible.</p>
<form  method="#">
<table>
<tr>
    <td><label for="name"><strong>Name:</strong></label></td>
	  </tr>
    <tr>
    <td><input type="text" name="name" id="textfield"></td>
    </tr>
    <tr>
    <td><label for="email"><strong>Email:</strong></label></td>
	  </tr>
    <tr>
    <td><input type="text" name="email" id="textfield"></td>
    </tr>
    <tr>
    <td><label for="message"><strong>Message:</strong></label></td>
	  </tr>
    <tr>
    <td><textarea type="text" name="message" rows="8"> </textarea></td>
	</tr>
    <tr>
    <td><%= link_to image_tag("/assets/contactbutton.png"), "#" %></td>
    </tr>
</table>
</form>
</div>
